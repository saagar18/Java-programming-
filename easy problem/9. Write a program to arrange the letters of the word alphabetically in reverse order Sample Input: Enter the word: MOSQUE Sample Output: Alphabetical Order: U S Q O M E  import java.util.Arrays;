public class Main {
    public static void main(String[] args) {
        String Word = "MOSQUE";
        char[] charArray = Word.toCharArray();
        Arrays.sort(charArray);
        String rev="";
        for(int i=charArray.length-1;i>=0;i--)
        {
            rev+=charArray[i];
        }
        System.out.println("Alphabetical Order in Reverse: " + rev);
    }
}
